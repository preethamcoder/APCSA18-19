public class Lincoln{
  
  //------------------------------------------------
  // Prints a presidential quote with double quotes
  //------------------------------------------------
  public static void main{
    System.out.println("A quote by Abraham Lincoln:");
    System.out.println("\"Whatever you are, be a good one.\"");
  }
}


