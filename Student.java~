public class Student{
  private String name;
  private int rollNo;
  private double gradeP;
  private int sub1Gr;
  private int sub2Gr;
  
  public Student(String naame, int rollNom, int sub1Gra, int sub2Gra, double gradePe){
    name = naame;
    rollNo = rollNom;
    sub1Gr = sub1Gra;
    sub2Gr = sub2Gra;
    gradeP = gradePe;
  }
  public double gpaRet{
    return("This person's GPA is "+((sub1Gr+sub2Gr)/2));  
  }
}